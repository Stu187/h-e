<wpml-config>
    <custom-fields>
	   <custom-field action="copy">featured_slide_redirect_url</custom-field>
	   <custom-field action="copy">featured_sslider_nolink</custom-field>
	   <custom-field action="copy">_featured_img_transition</custom-field>
	   <custom-field action="copy">_featured_img_duration</custom-field>
	   <custom-field action="copy">_featured_img_delay</custom-field>
	   <custom-field action="copy">_featured_title_transition</custom-field>
	   <custom-field action="copy">_featured_title_duration</custom-field>
	   <custom-field action="copy">_featured_title_delay</custom-field>
	   <custom-field action="copy">_featured_content_transition</custom-field>
	   <custom-field action="copy">_featured_content_duration</custom-field>
	   <custom-field action="copy">_featured_content_delay</custom-field>
	   <custom-field action="copy">featured_slider_thumbnail</custom-field>
           <custom-field action="translate">_featured_slide_nav</custom-field>
	   <custom-field action="copy">_featured_select_set</custom-field>
	   <custom-field action="copy">featured_link_attr</custom-field>
	   <custom-field action="copy">_featured_nav_thumb</custom-field>
	   <custom-field action="copy">_featured_disable_image</custom-field>
	   <custom-field action="copy">_featured_embed_shortcode</custom-field>
	   <custom-field action="copy">_featured_sldrdisplay</custom-field>
	   <custom-field action="copy">_featured_sldrnm</custom-field>
   </custom-fields>
    <custom-types>
        <custom-type translate="1">testimonial</custom-type>
        <custom-type translate="1">flashcard</custom-type>
	<custom-type translate="1">slidervilla</custom-type>
    </custom-types>    
    <taxonomies>
	 <taxonomy translate="1">testimonial_category</taxonomy>
	 <taxonomy translate="1">flashcard_category</taxonomy>
    </taxonomies>
    <admin-texts>
      	<key name="featured_slider_global_options">
            <key name="noscript" />
        </key>
    </admin-texts>    
    <language-switcher-settings>
        <key name="icl_lang_sel_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>
        <key name="icl_lang_sel_footer_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>    
        <key name="icl_language_switcher_sidebar">0</key>  
        <key name="icl_widget_title_show">0</key>
        <key name="icl_lang_sel_type">dropdown</key>
        <key name="icl_lso_link_empty">0</key>
        <key name="icl_lso_flags">0</key>
        <key name="icl_lso_native_lang">1</key>
        <key name="icl_lso_display_lang">1</key>
        <key name="icl_lang_sel_footer">0</key>
        <key name="icl_post_availability">0</key>
        <key name="icl_post_availability_position">below</key>
        <key name="icl_post_availability_text">This post is also available in: %s</key>    
    </language-switcher-settings>    
</wpml-config>


